services:
  librarymanager.api:
    build:
      context: E:\estudo C#\LibraryManager
      dockerfile: Dockerfile
      args:
        BUILD_CONFIGURATION: Release
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: LibraryManager.Api
    container_name: LibraryManager.Api
