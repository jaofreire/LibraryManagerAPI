services:
  librarymanager.api:
    container_name: libraryapi-container
    depends_on:
      mongo:
        condition: service_started
        required: true
      redis:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: jaofreire/library-api-image:latest
    networks:
      libraryapi-net: null
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
      - mode: ingress
        target: 8081
        published: "8081"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\joaoG\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\joaoG\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  mongo:
    container_name: mongo-container
    environment:
      MONGO_INITDB_ROOT_PASSWORD: js019283912839812391
      MONGO_INITDB_ROOT_USERNAME: jaomongo
    image: mongo:latest
    networks:
      libraryapi-net: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
  redis:
    container_name: redis-container
